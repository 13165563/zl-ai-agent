{
  "mcpServers": {
    "amap-maps": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@amap/amap-maps-mcp-server"
      ],
      "env": {
        "AMAP_MAPS_API_KEY": "c35d3d557f6de34f7ecae355b99d9147"
      }
    },
    "redbook-search": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@chenningling/redbook-search-mcp"
      ],
      "env": {
        "REDBOOK_API_KEY": "your_redbook_api_key_here"
      }
    },
    "z-image-search-mcp-server": {
      "command": "java",
      "args": [
        "-Dspring.ai.mcp.server.stdio=true",
        "-Dspring.main.web-application-type=none",
        "-Dlogging.pattern.console=",
        "-jar",
        "z-image-search-mcp-server/target/z-image-search-mcp-server-0.0.1-SNAPSHOT.jar"
      ],
      "env": {}
    }
  }
}
